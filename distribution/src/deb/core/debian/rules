#!/usr/bin/make -f
# -*- makefile -*-

export DEB_BUILD_OPTIONS = nocheck notest

#export DH_VERBOSE=1

# This has to be exported to make some magic below work.
export DH_OPTIONS
export OPTIMIZE=yes
export prefix=/usr
export DESTDIR=$(CURDIR)/debian/tmp

%:
	dh $@ --with python2

override_dh_auto_build:
	make embedded_runpath_prefix="" install
